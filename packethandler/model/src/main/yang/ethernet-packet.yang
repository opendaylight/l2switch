  typedef header8021q-type {
    type enumeration {
      enum "vlan-tagged" {
        value 33024; // 0x8100
        description "VLAN-tagged frame (IEEE 802.1Q) & Shortest Path Bridging IEEE 802.1aq[4]";
      }
      enum "q-in-q" {
        value 37120; // 0x9100
        description "Q-in-Q";
      }
    }
  }

  grouping header8021q {
    leaf type {
      type header8021q-type;
    }

    leaf priority-code {
      type int16;
    }

    leaf drop-eligible {
      type boolean;
    }

    leaf vlan {
      type int32;
    }
  }

  grouping ethernet-packet-grp {
    leaf source-mac {
      type yang:mac-address;
    }

    leaf destination-mac {
      type yang:mac-address;
    }

    list header8021q {
      uses header8021q;
    }

    leaf ethertype {
      type known-ether-type;
    }

    leaf ethernet-length {
      type int32;
    }

    leaf ethernet-payload {
      type binary;
    }

    uses packet:packet;
  }

  container ethernet-packet {
    uses ethernet-packet-grp;
  }

  /*grouping ipv4-packet {
    uses ethernet-packet;
  }

  grouping ipv6-packet {
    uses ethernet-packet;
  }

  grouping lldp-packet {
    uses ethernet-packet;
  }*/

  notification ethernet-packet-received {
    uses ethernet-packet-grp;
  }

}